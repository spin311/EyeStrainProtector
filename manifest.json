{
  "manifest_version": 3,
  "name": "Preserve vision",
  "version": "0.5.0",
  "description": "Google extension to notify you to rest your eyes every 20 minutes",

  "permissions": [
    "storage",
    "notifications",
    "offscreen",
    "alarms"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "title": "Preserve vision extension"
  },
  "icons": {
    "16": "imgs/icon.png",
    "48": "imgs/icon.png",
    "128": "imgs/icon.png"
  }
}
